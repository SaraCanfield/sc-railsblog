
<table border = "1">

<tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.fname %></td>
         <td><%= user.lname %></td>
          <td><%= user.email %></td>
        <td><a href="/users/<%= user.id %>">Show</a></td>
       

     	<td><%= link_to "Edit", edit_user_path(user) %></td>
        <td><%= link_to("Delete", user_path(user.id), method: :delete)%></td>
     
   
      </tr>

    <% end %>
    </tbody>
 </table>
 <br>

<%= link_to 'New User', new_user_path %>